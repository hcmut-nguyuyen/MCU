#ifndef INC_COMMAND_PARSER_H_
#define INC_COMMAND_PARSER_H_

typedef enum {
  COMMAND_INIT = 0,
  COMMAND_BEGIN,
  COMMAND_1_R,
  COMMAND_1_O,
  COMMAND_2_S,
  COMMAND_2_K,
  COMMAND_3_T,
  COMMAND_END
} CommandState;

#endif /* INC_COMMAND_PARSER_H_ */
